{
  "name": "@ec.com.smx.base/kng-web-base",
  "version": "1.0.0-SNAPSHOT",
  "registry": "docker.krugernetes.com/kruger",
  "projectname": "kng-web-base",
  "scripts": {
    "ng": "ng",
    "preinstall": "",
    "postinstall": "node version.js",
    "version": "echo $npm_package_version",
    "name": "echo $npm_package_name",
    "dockerRegistry": "echo $npm_package_publishConfig_dockerRegistry",
    "start": "ng serve --port 9004 --proxy-config proxy.conf.json  --live-reload",
    "build": "ng build --base-href ./",
    "build:pro": "ng build --configuration=pro --base-href /webbase/",
    "build:ci": "ng build --aot --build-optimizer --progress=false --configuration=pro --base-href /webbase/",
    "build:docker": "docker build -f $npm_package_publishConfig_dockerRegistry/ec.com.smx.base/kng-web-base:$npm_package_version -f ci/docker/DockerfileCI .",
    "test": "ng test --watch=false",
    "cover": "ng test --watch=false --browsers=ChromeHeadless --code-coverage=true",
    "test:security": "retire -j -p --proxy http://$http_proxy --colors --outputformat text --ignore node_modules/selenium-webdriver",
    "test:unit": "ng test --no-watch --browsers=ChromeHeadless --no-progress",
    "test:integration": "ng run kng-web-base:integration --no-watch --browsers=ChromeHeadless --no-progress",
    "lint:fix": "ng lint $npm_package_projectname --fix",
    "lint": "ng lint",
    "lint:ci": "eslint --format=checkstyle > checkstyle-result.xml 'src/app/**'",
    "e2e": "ng e2e",
    "sonar": "npm run cover && sonar-scanner",
    "build:war": "npm run build && cd dist && jar -cvf ../$npm_package_projectname-$npm_package_version.war *",
    "build:pro:war": "npm run build:pro && cd dist && jar -cvf ../$npm_package_projectname-pro-$npm_package_version.war *",
    "deploy:maven": "npm run build:pro:war && ts-node deploy.ts",
    "publish": "npm run build:pro && cd dist && npm publish",
    "publish:docker": "npm run build:docker && docker push $npm_package_publishConfig_dockerRegistry/ec.com.smx.base/kng-web-base:$npm_package_version",
    "build:single-spa:kng-web-base": "ng build kng-web-base --prod --deploy-url http://localhost:4200/",
    "serve:single-spa:kng-web-base": "ng s --project kng-web-base --disable-host-check --port 4201 --deploy-url http://localhost:4201/ --live-reload false"
  },
  "publishConfig": {
    "registry": "https://cf1nxs.cfavorita.net/repository/npm-internal/",
    "dockerRegistry": "cf1dck.cfavorita.net"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.2",
    "@angular/cdk": "10.2.7",
    "@angular/common": "^10.2.2",
    "@angular/compiler": "^10.2.2",
    "@angular/core": "^10.2.2",
    "@angular/elements": "^10.2.2",
    "@angular/forms": "^10.2.2",
    "@angular/platform-browser": "^10.2.2",
    "@angular/platform-browser-dynamic": "^10.2.2",
    "@angular/router": "^10.2.2",
    "@ec.com.kruger/kng-components": "4.0.11-rc",
    "@ng-select/ng-select": "^5.0.8",
    "@ngx-pwa/local-storage": "^10.1.0",
    "@popperjs/core": "^2.5.3",
    "bootstrap": "^4.4.1",
    "font-awesome": "4.7.0",
    "jquery": "3.5.1",
    "keycloak-angular": "^7.3.0",
    "keycloak-js": "^11.0.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "ng-block-ui": "3.0.2",
    "ng-sidebar": "9.4.2",
    "ngx-bootstrap": "^6.1.0",
    "ngx-toastr": "^13.1.0",
    "node-sass": "^4.12.0",
    "npm-font-open-sans": "^1.1.0",
    "open-sans-fontface": "1.4.0",
    "primeicons": "^4.0.0",
    "primeng": "^9.1.3",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.6.3",
    "single-spa": "^5.9.3",
    "single-spa-angular": "^4.9.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.1.0-beta.0",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "10.2.2",
    "@angular/language-service": "10.2.2",
    "@ec.com.kruger/eslint-config-kruger": "2.0.0",
    "@types/jasmine": "3.6.1",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^14.0.4",
    "@types/rx": "^4.1.2",
    "@typescript-eslint/parser": "^4.9.0",
    "codelyzer": "^6.0.1",
    "eslint": "^7.14.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "fs-extra": "9.0.1",
    "git-describe": "^4.0.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~7.0.0",
    "retire": "^2.2.3",
    "sonar-scanner": "^3.1.0",
    "ts-node": "^9.0.0",
    "typescript": "4.0.5"
  }
}
